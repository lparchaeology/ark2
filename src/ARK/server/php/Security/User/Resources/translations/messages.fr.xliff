        <source>Choose a new password, %name%</source>
        <target>Choisis un nouveau mot de passe, %name%</target>
      </trans-unit>
      <trans-unit id="messages-42">
        <source>Enter your email address below and we'll send you password reset instructions.</source>
        <target>Entre ton adresse e-mail ci-dessous pour recevoir les directives de réinitialisation de mot de passe.</target>
      </trans-unit>
      <trans-unit id="messages-43">
        <source>Reset</source>
        <target>Réinitialiser</target>
      </trans-unit>
      <trans-unit id="messages-44">
        <source>Thank you!</source>
        <target>Merci !</target>
      </trans-unit>
      <trans-unit id="messages-45">
        <source>No user was found with that ID.</source>
        <target>Aucun utilisateur trouvé.</target>
      </trans-unit>
      <trans-unit id="messages-46">
        <source>That user is disabled (pending email confirmation).</source>
        <target>Ce compte n'est pas activé (attendre l'e-mail de confirmation).</target>
      </trans-unit>
      <trans-unit id="messages-47">
        <source>Saved account information.</source>
        <target>Les paramètres de ce compte sont enregistrés.</target>
      </trans-unit>
      <trans-unit id="messages-48">
        <source>You need to be logged in.</source>
        <target>Tu dois être connecté.</target>
      </trans-unit>
