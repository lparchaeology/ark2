{
    "site": "dime",
    "debug": true,
    "timezone": "UTC",
    "locale": "da",
    "locales": ["en", "da"],
    "spatial": "mysql",
    "api": {
        "enable": true,
        "path": "/api/v2"
    },
    "web": {
        "enable": true,
        "frontend": "dime2017"
    },
    "security": {
        "access_rules": [
            ["(^/users)|(^/fund/add$)|(^/lokalitet/add$)", "ROLE_USER"],
            ["^/.+$", "IS_AUTHENTICATED_ANONYMOUSLY"]
        ]
    }
}
