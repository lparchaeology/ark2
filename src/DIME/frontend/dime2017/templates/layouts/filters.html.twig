{{ form_start(form) }}
<div class="col-md-1">
    <br/>
    {{ 'dime.find.filters' | translate }}
</div>
<div class="col-sm-10">
    <div class="row">
        {% for key, subform in form if key != '_token' and key != 'search'%}
            <div class="col-sm-3">
                <div class="form-group">
                    {{- form_label(subform) -}}<br/>
                    {{- form_widget(subform) -}}
                    {% do subform.setRendered %}
                </div>
            </div>
        {% endfor %}
    </div>
</div>
<div class="col-md-1">
    <br/>
    {{- form_widget(form.search) -}}
    {% do form.search.setRendered %}
</div>
{{ form_end(form) }}
